  }
   void jButton3_actionPerformed(ActionEvent e) {
    if (sh <=7) sh++;
    if ((it ==3)&&(sh>7)) sh=8;
    if ((sh >6)&&(it <3)) {
      sh=3;
      it++;
      }
    if (it >3) {it=3; sh=8;}
    repaint();
   }

  void jButton1_actionPerformed(ActionEvent e) {
    if ((it ==0)&&(sh<=4))   sh--;
    if ((it ==0)&&(sh <=3)) sh=sh+1;
    sh--;
    if ((sh <3)&&(it !=0))
     { sh=6;
       it--;}
    if(sh < 1) sh=1;
    if (it < 0) it=0;
     repaint();
  }

  void jButton2_actionPerformed(ActionEvent e) {
    sh=1; it=0;
    repaint();
  }

